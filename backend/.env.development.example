NODE_ENV=development
PORT=3001
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Dev auth profile (RS256 only)
JWT_ALLOWED_ALGS=RS256
# Escape line breaks with \n when storing PEM in .env files.
JWT_PUBLIC_KEY="-----BEGIN PUBLIC KEY-----\nREPLACE_WITH_RSA_PUBLIC_KEY\n-----END PUBLIC KEY-----"
JWT_ISSUER=slotsone-dev
JWT_AUDIENCE=slotsone-client
